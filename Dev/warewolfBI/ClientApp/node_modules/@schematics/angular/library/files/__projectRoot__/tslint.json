{
    "extends": "<%= relativePathToWorkspaceRoot %>/tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "<%= prefix %>",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "<%= prefix %>",
            "kebab-case"
        ]
    }
}
