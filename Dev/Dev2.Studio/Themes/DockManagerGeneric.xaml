					<Border x:Name="topIndicatorIconShadow" Background="{DynamicResource {x:Static igDock:DockManagerBrushKeys.LightBrushKey}}" CornerRadius="4,4,0,0" Margin="0,0,0,17" SnapsToDevicePixels="True"/>
