<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:controls="clr-namespace:Infragistics.Windows.Controls"
    xmlns:igWindows="clr-namespace:Infragistics.Windows.Controls"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:PresentationOptions="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
		xmlns:themes="clr-namespace:Infragistics.Windows.Themes"
    >
	<!--<ResourceDictionary.MergedDictionaries>
		<themes:PrimitivesGeneric/>
	</ResourceDictionary.MergedDictionaries>-->

	<LinearGradientBrush PresentationOptions:Freeze="true" x:Key="{ComponentResourceKey {x:Type controls:IGControlBase}, CarouselItemSelected}" StartPoint="0,0" EndPoint="0,1">
		<GradientStop Color="#FF969696" Offset="0.985"/>
		<GradientStop Color="#FFF5F5F5" Offset="0"/>
	</LinearGradientBrush>
    <SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igWindows:PrimitivesBrushKeys.TabControlTabStripCenterFillKey}" themes:ResourceWasher.WashGroup="BaseColor" Color="Transparent"/>


    <!-- ===================== DropIndicator Brushes ===================== -->
    <LinearGradientBrush PresentationOptions:Freeze="true" 
				x:Key="{x:Static igWindows:PrimitivesBrushKeys.DropIndicatorFillKey}"
			StartPoint="0.5,0"
			EndPoint="0.5,1">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop
						Color="#FF5D5D5D"
						Offset="0" />
                <GradientStop
						Color="#FF3C3C3C"
						Offset="1" />
            </GradientStopCollection>
        </LinearGradientBrush.GradientStops>
    </LinearGradientBrush>
    <SolidColorBrush PresentationOptions:Freeze="true" x:Key="{x:Static igWindows:PrimitivesBrushKeys.DropTargetStrokeKey}" Color="#FFB0AFA0"/>

    <!-- ===================== END DropIndicator Brushes ===================== -->
</ResourceDictionary>