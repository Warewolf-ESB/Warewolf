<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:igwindows="http://infragistics.com/Windows"
	xmlns:editors="http://infragistics.com/Editors"	
	xmlns:controls="http://infragistics.com/DataPresenter"
	xmlns:igThemes="http://infragistics.com/Themes"
    >
    <ResourceDictionary.MergedDictionaries>
        <!--<igThemes:ResourceSetLoader Locator="{x:Static igThemes:EditorsGeneric.Location}"/>-->
        <igThemes:ResourceSetLoader Locator="{x:Static igThemes:EditorsWashBaseLight.Location}"/>

        <igThemes:ResourceWasher AutoWash="True" WashMode="HueSaturationReplacement" >
            <igThemes:ResourceWasher.SourceDictionary>
                <igThemes:EditorsWashBaseLightBrushes/>
            </igThemes:ResourceWasher.SourceDictionary>
            <igThemes:ResourceWasher.WashGroups>
                <igThemes:WashGroupCollection>
                    <igThemes:WashGroup Name="ThemeColor"  WashColor="#FFE13E7D"/>
                    <igThemes:WashGroup Name="AlternateColor" WashColor="#FFE45E9D"  />
                </igThemes:WashGroupCollection>
            </igThemes:ResourceWasher.WashGroups>
        </igThemes:ResourceWasher>
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>