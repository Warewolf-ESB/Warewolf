                                    <Border x:Name="ActiveResizeBorder" Margin="1,1,1,1" Background="{StaticResource ActiveResizeBorderBrush}" CornerRadius="4,4,0,0" Opacity="0.65"/>
                                    <Border BorderBrush="White" BorderThickness="1" CornerRadius="4,4,0,0" Margin="1" Opacity="0.7"/>
                                </Grid>
                                <Grid x:Name="NoActiveResizeBorderGrid" Margin="0" Visibility="Collapsed" Background="{x:Null}">
                                    <Border x:Name="NoActiveResizeBorder" Margin="1,1,1,1" Background="{StaticResource ActiveResizeBorderBrush}" CornerRadius="4,4,0,0" Opacity="0.30" />
                                </Grid>
                            </Grid>

                            <ContentControl x:Name="MaximizedContent" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" IsTabStop="False">
                                <Grid>

                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" MinHeight="26"/>
                                        <RowDefinition />
                                    </Grid.RowDefinitions>

                                    <!-- Content -->
                                    <Grid Margin="6,0,6,6" x:Name="ContentGrid" Grid.Row="1">
                                        <Border BorderBrush="#FFFFFFFF" BorderThickness="1" CornerRadius="1"/>
                                        <Border Margin="1" BorderThickness="1" BorderBrush="{StaticResource MainBrushColor}"/>
                                        <Border Margin="1" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" BorderBrush="{StaticResource DarkBrush}">
                                            <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                                <Grid x:Name="Core" DataContext="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.CurrentBadWord}" Margin="{TemplateBinding Padding}">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                        <RowDefinition Height="Auto"/>
                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="290"/>
                                                        <ColumnDefinition Width="110"/>
                                                    </Grid.ColumnDefinitions>
                                                    <StackPanel Orientation="Horizontal">
                                                        <TextBlock Margin="0,5,0,5" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.NotInDictionary}" TextWrapping="Wrap"/>
                                                        <TextBlock Margin="5,5,0,5" Text="{Binding Word}"/>
                                                    </StackPanel>
                                                    <StackPanel Grid.Row="1">
                                                        <TextBlock Margin="0,5,0,0" VerticalAlignment="Top" RenderTransformOrigin="1,1" Grid.Row="1" Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Context}" TextWrapping="Wrap"/>
                                                        <Border Margin="0,5,5,5" Background="#FFFFFFFF" BorderBrush="{StaticResource SpellCheckerBorderBrush}" BorderThickness="1" CornerRadius="1">
                                                            <ScrollViewer Height="80" BorderThickness="0">
                                                                <TextBlock x:Name="Context" TextWrapping="Wrap"/>
                                                            </ScrollViewer>
                                                        </Border>
                                                    </StackPanel>

                                                    <StackPanel Grid.Row="2">
                                                        <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Suggestions}" TextWrapping="Wrap" Margin="0,5,0,0"/>
                                                        <ListBox x:Name="Suggestions" Height="81" Margin="0,5,5,5" SelectedItem="{Binding Mode=TwoWay, ElementName=ChangeTo, Path=Text}"/>
                                                        <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.UseThisInstead}" Margin="0,5,0,0"/>
                                                        <TextBox x:Name="ChangeTo" TextWrapping="Wrap" Margin="0,5,5,5"/>
                                                    </StackPanel>

                                                    <StackPanel Margin="0,5,0,5" Grid.Row="3" Orientation="Horizontal">
                                                        <Button x:Name="PreviousField" HorizontalAlignment="Stretch" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.PreviousField}" Width="100">
                                                            <igBase:Commanding.Command>
                                                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="PreviousField"/>
                                                            </igBase:Commanding.Command>
                                                        </Button>
                                                        <Button x:Name="NextField" HorizontalAlignment="Stretch" Margin="5,0,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.NextField}" Width="100">
                                                            <igBase:Commanding.Command>
                                                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="NextField" />
                                                            </igBase:Commanding.Command>
                                                        </Button>
                                                    </StackPanel>

                                                    <StackPanel Margin="5,10,5,5" Grid.Column="1" Grid.Row="1">
                                                        <Button x:Name="Ignore" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ignore}" >
                                                            <igBase:Commanding.Command>
                                                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="Ignore" />
                                                            </igBase:Commanding.Command>
                                                        </Button>
                                                        <Button x:Name="IgnoreAll" Margin="0,5,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.IgnoreAll}">
                                                            <igBase:Commanding.Command>
                                                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="IgnoreAll" />
                                                            </igBase:Commanding.Command>
                                                        </Button>
                                                    </StackPanel>

                                                    <StackPanel Margin="5,26,5,5" Grid.Column="1" Grid.Row="2">
                                                        <Button x:Name="Change" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Change}">
                                                            <igBase:Commanding.Command>
                                                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="Change" />
                                                            </igBase:Commanding.Command>
                                                        </Button>
                                                        <Button x:Name="ChangeAll" Margin="0,5,0,0" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.ChangeAll}">
                                                            <igBase:Commanding.Command>
                                                                <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="ChangeAll" />
                                                            </igBase:Commanding.Command>
                                                        </Button>
                                                    </StackPanel>

                                                    <Button Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ok}" Grid.Row="3" Grid.Column="1" Margin="5">
                                                        <igBase:Commanding.Command>
                                                            <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="CancelDialog" />
                                                        </igBase:Commanding.Command>
                                                    </Button>

                                                </Grid>

                                                <Grid x:Name="SpellCheckCompleteGrid" Visibility="Collapsed" Margin="{TemplateBinding Padding}" HorizontalAlignment="Center" VerticalAlignment="Center">
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition/>
                                                        <RowDefinition/>
                                                    </Grid.RowDefinitions>
                                                    <TextBlock Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.SpellCheckComplete}" HorizontalAlignment="Center" Margin="10,5"></TextBlock>
                                                    <Button Width="100" Content="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=SpellChecker.DialogSettings.DialogStringResources.Ok}" Grid.Row="1" Margin="10,5" >
                                                        <igBase:Commanding.Command>
                                                            <ig:XamSpellCheckerDialogCommandSource EventName="Click" CommandType="CloseDialog" />
                                                        </igBase:Commanding.Command>
                                                    </Button>
                                                </Grid>
                                            </Grid>
                                        </Border>
                                    </Grid>

                                    <!-- Header -->
                                    <Border x:Name="HeaderArea" Background="{TemplateBinding HeaderBackground}" CornerRadius="5,5,0,0" Margin="1,1,1,0"/>

                                    <Grid x:Name="WindowHeader"  Margin="1,1,105,0" VerticalAlignment="Center">
                                        <Grid HorizontalAlignment="Stretch" >
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <ContentControl Grid.Column="0" x:Name="SmallImage"
												ContentTemplate="{TemplateBinding HeaderIconTemplate}"
												MinHeight="16" MinWidth="16" MaxHeight="16" MaxWidth="16"
												Visibility="Visible" Margin="5,0,0,0" IsTabStop="False" VerticalAlignment="Center"/>

                                            <ContentControl Grid.Column="2" Margin="5,0,0,0" HorizontalAlignment="Stretch"
												 Content="{TemplateBinding Header}"
												 ContentTemplate="{TemplateBinding HeaderTemplate}"
												 Foreground="{TemplateBinding HeaderForeground}" IsHitTestVisible="False"
												 HorizontalContentAlignment="{TemplateBinding HeaderHorizontalContentAlignment}"
												 VerticalContentAlignment="{TemplateBinding HeaderVerticalContentAlignment}"
												 IsTabStop="False"/>
                                        </Grid>
                                    </Grid>

                                    <ContentControl x:Name="MinimizedContent" Visibility="Collapsed" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch"
												Width="{TemplateBinding MinimizedWidth}" Height="{TemplateBinding MinimizedHeight}"
												IsTabStop="False" >
                                        <Grid x:Name="MinimizedContentPanel"  Margin="1,1,1,0" HorizontalAlignment="Stretch" VerticalAlignment="Top" >
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="Auto"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <ContentControl x:Name="MinimizedSmallImage"
												ContentTemplate="{TemplateBinding HeaderIconTemplate}" Grid.Column="0"
												MinHeight="16" MinWidth="16" MaxHeight="16" MaxWidth="16"
												Visibility="Visible" IsTabStop="False"/>

                                            <ContentControl Margin="7,3,8,0"
												HorizontalAlignment="Stretch" Grid.Column="1"
												Content="{TemplateBinding Header}"
												ContentTemplate="{TemplateBinding MinimizedHeaderTemplate}"
												Foreground="{TemplateBinding HeaderForeground}" IsHitTestVisible="False"
												HorizontalContentAlignment="{TemplateBinding HeaderHorizontalContentAlignment}"
												VerticalContentAlignment="{TemplateBinding HeaderVerticalContentAlignment}"
												IsTabStop="False"/>

                                        </Grid>
                                    </ContentControl>
                                    <!--</Border>-->

                                </Grid>
                                <!--</ContentControl>-->

                            </ContentControl>

