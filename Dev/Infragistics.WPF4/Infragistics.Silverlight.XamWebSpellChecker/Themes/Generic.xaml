                        </VisualStateManager.VisualStateGroups>
                        <ig:SpellCheckerDialogRootPanel x:Name="Root">

                            <!-- Borders -->
                            <Grid x:Name="BorderGrid">
                                <Border x:Name="Border1" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="1,1,1,1" CornerRadius="5,5,0,0" Opacity="0.65"/>
