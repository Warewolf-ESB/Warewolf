<?xml version="1.0" encoding="UTF-8"?>
<TestSettings name="Integration Test Settings" id="e8eace60-accd-4300-bc1d-1c967914da3a" xmlns="http://microsoft.com/schemas/VisualStudio/TeamTest/2010">
  <Description>These are default test settings for a local test run.</Description>
    <Deployment>
        <DeploymentItem filename="TestBinaries\" />
        <DeploymentItem filename="Dev2.Server\bin\Debug\" outputDirectory="DebugBin" />
	</Deployment>
  <NamingScheme baseName="TestResults" appendTimeStamp="false" />
