{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launcher (AdminMode)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Warewolf.Launcher/bin/Debug/Warewolf.Launcher.exe",
            "args": ["--AdminMode"],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch Server (ServerTests)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Warewolf.Launcher/bin/Debug/Warewolf.Launcher.exe",
            "args": ["--StartServer", "--ResourcesType \"ServerTests\""],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Launch Server+Studio (UITests)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Warewolf.Launcher/bin/Debug/Warewolf.Launcher.exe",
            "args": ["--StartStudio", "--ResourcesType \"UITests\""],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Compile AcceptanceTesting",
            "script": "${workspaceFolder}/../Compile.ps1",
            "args": ["-AcceptanceTesting", "-MSBuildPath 'C:\\Program Files (x86)\\Microsoft Visual Studio\\2017\\Enterprise\\MSBuild\\15.0\\Bin\\MSBuild.exe'"],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "PowerShell",
            "request": "launch",
            "name": "PowerShell Compile UITesting",
            "script": "${workspaceFolder}/../Compile.ps1",
            "args": ["-UITesting", "-MSBuildPath 'C:\\Program Files (x86)\\Microsoft Visual Studio\\2017\\Enterprise\\MSBuild\\15.0\\Bin\\MSBuild.exe'"],
            "cwd": "${workspaceFolder}"
        }
    ]
}